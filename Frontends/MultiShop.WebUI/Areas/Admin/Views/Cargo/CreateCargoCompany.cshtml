@model CreateCargoCompanyDto
@{
    ViewData["Title"] = "Kargo Ekleme";
    ViewData["v1"] = "Anasayfa";
    ViewData["v2"] = "Kargolar";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <div class="bs-example" data-example-id="bordered-table">
                    <form method="post">
                        <label>Kargo Şirketi Adı</label>
                        <input type="text" class="form-control" asp-for="cargoCompanyName" />
                        <br />
                        <button class="btn btn-primary">Kargo Şirketini Ekle</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>

